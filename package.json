{
  "name": "react.rx",
  "version": "1.1.0",
  "description": "react with rxjs",
  "main": "build/index.js",
  "scripts": {
    "example": "npm run build && webpack --config example/webpack.js",
    "build": "babel lib --out-dir build",
    "prepublish": "npm run build",
    "lint": "eslint lib example",
    "test": "jest"
  },
  "repository": "coderhaoxin/react.rx",
  "keywords": [
    "reactjs",
    "rxjs"
  ],
  "files": [
    "build/",
    "lib/"
  ],
  "author": "haoxin",
  "license": "MIT",
  "dependencies": {
    "clone": "2",
    "immutability-helper": "2",
    "rxjs": "5",
    "uuid": "3"
  },
  "devDependencies": {
    "babel-cli": "6",
    "babel-core": "6",
    "babel-eslint": "7",
    "babel-jest": "17",
    "babel-loader": "6",
    "babel-plugin-transform-class-properties": "6",
    "babel-plugin-transform-decorators-legacy": "1",
    "babel-plugin-transform-object-rest-spread": "6",
    "babel-preset-es2015": "6",
    "babel-preset-react": "6",
    "eslint": "3",
    "eslint-config-bx-bsd": "1",
    "eslint-plugin-react": "6",
    "jest": "19",
    "react": "15",
    "jest-cli": "17",
    "react-addons-test-utils": "15",
    "react-dom": "15",
    "webpack": "2"
  },
  "jest": {
    "collectCoverageFrom": ["lib/*.js"],
    "collectCoverage": true,
    "testPathDirs": [
      "test"
    ],
    "testRegex": "js$",
    "verbose": false,
    "bail": true
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-bx-bsd"
    ]
  }
}
